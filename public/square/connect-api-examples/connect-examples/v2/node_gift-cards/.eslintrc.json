{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "off"
    },
    "globals": {
      "process": true,
      "require": true,
      "module": true,
      "bwipjs": true
    }
}
